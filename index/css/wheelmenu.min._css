.wheel-button{position:relative}.wheel{margin:0;padding:0;list-style:none;width:270px;height:270px;visibility:hidden;position:relative;display:none;z-index:999999}.wl-mini{display:block;width:94px;height:94px;background:url(../images/wheel-1.png) no-repeat center center;background-size:58px 58px}.wheel-button,.wheel-button:visited{background:#cfcfd0;text-align:center;border-radius:50px;width:65px;height:65px;display:block;box-shadow:0 1px 2px rgba(0,0,0,0.25);-moz-box-shadow:0 1px 2px rgba(0,0,0,0.25);-webkit-box-shadow:0 1px 2px rgba(0,0,0,0.25)}.wheel-button:hover{color:white}.wheel-button.ne{border-color:white;background:#1abc9c;color:#34ffff;position:absolute;bottom:10px;left:10px;z-index:999999}.wheel-button.nw{width:94px;height:94px;background:#cfcfd0;background:rgba(207,207,207,0.07);filter:Alpha(opacity=7);position:fixed;top:230px;right:60px;z-index:999999}.wheel-button span,.wheel span{position:relative;-moz-transition:all 1s ease;-webkit-transition:all 1s ease;-o-transition:all 1s ease;transition:all 1s ease;display:block}.wheel-button.active span{transform:rotate(135deg);-ms-transform:rotate(135deg);-webkit-transform:rotate(135deg)}.wheel li a,.wheel li a:visited{border-radius:50px;text-align:center;width:65px;height:65px;box-shadow:0 1px 2px rgba(0,0,0,0.25),inset 0 1px 1px rgba(255,255,255,0.5);-moz-box-shadow:0 1px 2px rgba(0,0,0,0.25),inset 0 1px 1px rgba(255,255,255,0.5);-webkit-box-shadow:0 1px 2px rgba(0,0,0,0.25),inset 0 1px 1px rgba(255,255,255,0.5);color:white;-moz-transition:all .25s ease;-webkit-transition:all .25s ease;-o-transition:all .25s ease;transition:all .25s ease}.wheel li a{display:block;background-size:100%}.wheel li{overflow:hidden;float:left;width:65px;height:65px}.wheel li.item1 a{background:url(../images/wheel-6.png) no-repeat center center;background-size:65px}.wheel li.item2 a{background:url(../images/wheel-7.png) no-repeat center center;background-size:65px}.wheel li.item3 a{background:url(../images/wheel-4.png) no-repeat center center;background-size:65px}.wheel li.item4 a{background:url(../images/wheel-3.png) no-repeat center center;background-size:65px}.wheel li.item5 a{background:url(../images/wheel-2.png) no-repeat center center;background-size:65px}.wheel li.item6 a{background:url(../images/wheel-5.png) no-repeat center center;background-size:65px}.wheel li.item6 a{background:url(../images/wheel-8.png) no-repeat center center;background-size:65px}